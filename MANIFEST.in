include LICENSE
include README.md
include CODE_OF_CONDUCT.md
include CONTRIBUTING.md

include setup.cfg
include pyproject.toml
include Makefile

# grab js
graft js
prune js/coverage
prune js/dist
prune js/docs
prune js/lib
prune js/node_modules
prune js/package-lock.json
prune js/yarn.lock

# Get rid of docs
prune docs

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
